void main() {
  // Current time
  int currentHours = 14;
  int currentMinutes = 34;
  int currentSeconds = 42;


  int total = 24 * 60 * 60;

 
  int second = (currentHours * 60 * 60) + (currentMinutes * 60) + currentSeconds;

  int remainingSeconds = total - second;

  print("Remaining seconds in the day: $remainingSeconds");
}
